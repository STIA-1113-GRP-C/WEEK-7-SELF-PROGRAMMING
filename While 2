package selfProgramming;

public class example_2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int sum = 0, num = 1;
		
		while (num <= 10) {
			sum = sum + num;
		    num = num + 5;
		    System.out.println("SUM : " + sum);
		}    
        System.out.println("Good Bye");
	}

}
