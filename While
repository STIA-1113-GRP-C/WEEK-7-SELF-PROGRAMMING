package myWhile;

public class noteSlide_6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i = 1;
		int j = 10;
		int k = 20;
		
		while (i < 5) {
			System.out.println(i + "");
			i++;
		}
		while (j < 20) {
			System.out.println(j + "");
			j++;
		}
		while (k > 10) {
			System.out.println(k + "");
			k--;
		}

	}

}
