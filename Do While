package myWhile;

public class noteSlide_7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i = 1;
		int j = 10;
		int k = 20;
		
		do {
			System.out.println(i + "");
			i++;
		}while (i < 5);
		
		do {
			System.out.println(j + "");
			j++;
		}while (j < 20);
		
		do {
			System.out.println(k + "");
			k--;
		}while (k > 10);
	}

}
